{
  "name": "@dankreiger/react-profiler-table",
  "version": "1.4.3",
  "description": "React component for printing tables with Profiler API",
  "keywords": [
    "react",
    "profiler",
    "debug",
    "performance"
  ],
  "homepage": "https://github.com/dankreiger/react-profiler-table#readme",
  "bugs": {
    "url": "https://github.com/dankreiger/react-profiler-table/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dankreiger/react-profiler-table.git"
  },
  "license": "ISC",
  "author": "Dan Kreiger",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn clean && rollup -c ./rollup.config.ts",
    "clean": "rimraf ./dist",
    "semantic-release": "semantic-release",
    "test": "jest",
    "coverage": "jest --coverage",
    "prepare": "npx husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,jsx,json,css,md}": [
      "prettier --single-quote --write"
    ],
    "*.md,*.yml": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@babel/preset-typescript": "7.13.0",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.10",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.2",
    "husky": "5.2.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.40.0",
    "rollup-plugin-insert": "1.3.1",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "semantic-release": "17.4.1",
    "ts-jest": "26.5.3",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  }
}
